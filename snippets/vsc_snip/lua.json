{
  "lua_file_header": {
    "prefix": "hdm",
    "body": [
      "--================================================================",
      "-- ${TM_FILENAME}",
      "--================================================================",
      "-- ${RELATIVE_FILEPATH}",
      "-- created: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "",
      ""
    ],
    "description": "File header"
  },
  "Comments Header": {
    "prefix": "hd",
    "body": [
      "------------------------------------------------------------------",
      "-- $1",
      "------------------------------------------------------------------",
      "$2"
    ]
  }
}
